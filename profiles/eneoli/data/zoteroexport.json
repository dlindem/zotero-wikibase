    "data": {
      "key": "TEJKZWZS",
      "version": 26466,
      "itemType": "conferencePaper",
      "title": "On Italian Lexical Blends: Borrowings, Hybridity, Adaptations, and Native Word Formations",
      "creators": [
        {
          "creatorType": "editor",
          "firstName": "Sebastian",
          "lastName": "Knospe"
        },
        {
          "creatorType": "editor",
